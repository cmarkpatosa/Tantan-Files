<div class="modal fade" id="modal-import">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Import File</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <a href="~/PRTemplate.xlsx">Download Template</a>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Upload File</label>
                            <input type="file" class="form-control" id="fileUpload" />
                        </div>
                    </div>
                </div>
                @*<div class="row">
                    <div class="col-md-12">
                        <h6 class="bg-red">Note: When filling up date column kindly ensure to follow the required format (mm/dd/yyyy)</h6>
                    </div>
                </div>*@
            </div>
            <div class="modal-footer justify-content-between">
                <button type="button" data-dismiss="modal" class="btn btn-default">Close</button>
                <button type="button" id="btnUpload" class="btn btn-primary">Upload</button>
            </div>
        </div>
    </div>
</div>
